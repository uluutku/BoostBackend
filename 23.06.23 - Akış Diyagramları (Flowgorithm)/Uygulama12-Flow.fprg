                    <then>
                        <assign variable="kisiSayisi" expression="kisiSayisi + 1"/>
                    </then>
                    <else/>
                </if>
                <assign variable="sayac" expression="sayac + 1"/>
                <output expression="&quot;Ki&#351;i kay&#305;t edildi.&quot;" newline="True"/>
            </while>
            <output expression="&quot;Tak&#305;mda 18 - 25 ya&#351; aral&#305;&#287;&#305;ndaki ki&#351;i say&#305;s&#305;: &quot; &amp; kisiSayisi" newline="True"/>
        </body>
